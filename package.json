{
  "name": "qiankun-react-example",
  "version": "0.0.1",
  "description": "qiankun demo",
  "main": "index.js",
  "scripts": {
    "install": "npm-run-all --serial install:*",
    "install:main": "cd main && npm i",
    "install:sub-react": "cd sub-react-app && npm i",
    "install:sub-react2": "cd sub-react-app2 && npm i",
    "install:sub-vue3": "cd sub-vue3-app && npm i",
    "start": "npm-run-all --parallel start:*",
    "start:main": "cd main && npm start",
    "start:sub-react": "cd sub-react-app && npm start",
    "start:sub-react2": "cd sub-react-app2 && npm start",
    "start:sub-vue3": "cd sub-vue3-app && npm start",
    "build": "npm-run-all build:* && bash ./scripts/bundle.sh",
    "build:main": "cd main && npm run build",
    "build:sub-react": "cd sub-react-app && npm run build",
    "build:sub-react2": "cd sub-react-app2 && npm run build",
    "build:sub-vue3": "cd sub-vue3-app && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {},
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:tanwei0425/qiankun-react-example.git"
  },
  "keywords": [
    "qiankun",
    "react18",
    "vue3",
    "react",
    "vue",
    "qiankun-example",
    "qiankun-demo",
    "javascript"
  ],
  "author": "tanwei",
  "license": "MIT"
}
